    <validationRules>
        <fullName>PredictorValueMandatory</fullName>
        <active>true</active>
        <description>If the name of the predictor is SKU or Segment it must have a predictor value.</description>
        <errorConditionFormula>AND(
OR(
 ISPICKVAL(PredictorName__c , &quot;SKU&quot;),
 ISPICKVAL(PredictorName__c , &quot;Segment&quot;) 
),
ISBLANK(TEXT(PredictorValue__c) ) 
)</errorConditionFormula>
        <errorMessage>If the name of the predictor is SKU or Segment it must have a predictor value.</errorMessage>
    </validationRules>
