        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISBLANK(Other_Vendorsnew__c),
ISPICKVAL(Win_Validation__c,&quot;Pending Review&quot;),
OR(
RecordType.Name=&quot;Sales Opportunity&quot;,
RecordType.Name=&quot;Renewal&quot;))</errorConditionFormula>
        <errorMessage>Please make sure that &quot;Other Vendors&quot; in the Account is filled in before setting the Win Validation to Pending Review.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Products_Required_to_Save_at_Stage_4</fullName>
        <active>true</active>
        <description>Requires user to add products to the Opportunity in order to save the Opportunity at a stage 4 or higher.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
 OR(
  RecordType.DeveloperName = &quot;Sales Opportunity&quot; ,
ISPICKVAL(StageName, &quot;Closed Won&quot;)
),
Product_Count__c =0
)</errorConditionFormula>
        <errorMessage>You must add Products to the Opportunity before Closing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Referral_Blank_On_Upsells</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
OR(
RecordType.Name = &quot;Sales Opportunity&quot;,
RecordType.Name = &quot;Sales Opportunity - Closed&quot;,
RecordType.Name = &quot;Renewal&quot;,
RecordType.Name = &quot;Renewal - Closed&quot;),
Upsells__c &gt; 0,
ISBLANK(TEXT(Referral_Type__c)),
NOT(ISBLANK(TEXT(Win_Validation__c))))</errorConditionFormula>
        <errorDisplayField>Referral_Type__c</errorDisplayField>
        <errorMessage>To be deleted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RenewalsFields_Reqd_to_Save_Stage_5</fullName>
        <active>false</active>
        <description>Sales Opportunity Stage 5 Requirements</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
  RecordType.Name  = &quot;Renewal&quot;,
 ISPICKVAL( Account.CRE_Exposure__c , &quot; &quot;),
OR(
ISPICKVAL(StageName, &quot;Agreement Sent and Being Negotiated (5)&quot;),
ISPICKVAL(StageName, &quot;Verbal Win (6)&quot;),
ISPICKVAL(StageName, &quot;Closed Won&quot;)
))</errorConditionFormula>
        <errorMessage>to be deleted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Rep_Forecast_Category_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.Name = &quot;Sales Opportunity&quot;,
NOT(ISPICKVAL(StageName ,&quot;Qualified Prospect (2)&quot;)),
ISCHANGED(CloseDate),
ISBLANK(TEXT(Rep_Forecast_Category__c)),
CloseDate  &lt;  TODAY() + 89)</errorConditionFormula>
        <errorDisplayField>Rep_Forecast_Category__c</errorDisplayField>
        <errorMessage>This Opportunity is within the 90 day forecast period. Please update the Rep Forecast Category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_for_Subscription_SS6</fullName>
        <active>true</active>
        <description>To move to Stage 6, Contract Start/End Date, Billing Schedule and Billing Contact are required.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
 ISPICKVAL( StageName , &quot;Verbal Win (6)&quot;),
OR(
 ISBLANK(  Contract_Start_Date__c  ),
 ISBLANK(  Contract_End_Date__c  ),
 ISPICKVAL(  Billing_Schedule__c  , &quot;&quot;)),
OR(
 RecordType.Name = &quot;Renewal&quot;,
 RecordType.Name = &quot;Sales Opportunity&quot;)
)</errorConditionFormula>
        <errorMessage>Please enter Contract Start and End Date, Billing contact and Billing Frequency in order to save this Opportunity as a Verbal Win.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Stage4_AskPrice</fullName>
        <active>true</active>
        <description>Ask Price is required to save at ss4 or higher.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
OR(
RecordType.Name = &quot;Sales Opportunity&quot;,
RecordType.Name = &quot;Renewal&quot;),
OR(
ISPICKVAL(StageName, &quot;Pricing Proposal Sent (4)&quot;),
ISPICKVAL(StageName, &quot;Agreement Sent and Being Negotiated (5)&quot;),
ISPICKVAL(StageName, &quot;Verbal Win (6)&quot;),
ISPICKVAL(StageName, &quot;Closed Won&quot;)),
Evergreen_Contract__c = FALSE,
Current_Contract_Year__c = 1,
ISBLANK(Initial_Asking_Price__c ))</errorConditionFormula>
        <errorDisplayField>Initial_Asking_Price__c</errorDisplayField>
        <errorMessage>In order to move to Stage 4, please fill in the opportunity &quot;Initial Asking Price&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Risk_Reason_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.Name = &quot;Renewal&quot;,
 ISBLANK( Risk_Reason__c ) ,
 NOT(ISBLANK(TEXT( Risk_Type__c ))))</errorConditionFormula>
        <errorDisplayField>Risk_Reason__c</errorDisplayField>
        <errorMessage>Please fill in the Risk Reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Risk_Type_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.Name = &quot;Renewal&quot;,
ISBLANK(TEXT(Risk_Type__c)),
OR(
ISPICKVAL(Renewal_Risk__c,&quot;Difficult Renewal / BBFTD&quot;),
ISPICKVAL(Renewal_Risk__c,&quot;Consistently Unresponsive&quot;),
ISPICKVAL(Renewal_Risk__c,&quot;Known Change in Business Model&quot;),
ISPICKVAL(Renewal_Risk__c,&quot;Power User / VIP left the Firm&quot;)))</errorConditionFormula>
        <errorDisplayField>Risk_Type__c</errorDisplayField>
        <errorMessage>Please fill in the Risk Type if Renewal Risk is populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tracking_Consolidated_Opportunities</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISBLANK( Consolidated_To__c ) ,
 ISPICKVAL( StageName , &quot;Consolidated&quot;) )</errorConditionFormula>
        <errorMessage>Please make sure that &quot;Consolidated To&quot; is filled in before setting the Stage to &quot;Consolidated&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Win_Validation</fullName>
        <active>true</active>
        <description>Win Validation Required for renewals and sales ops</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
OR(
RecordType.Name = &quot;Sales Opportunity&quot;,
RecordType.Name = &quot;Renewal&quot;),
NOT(ISPICKVAL(Win_Validation__c, &quot;Opportunity Reviewed&quot;)),
ISPICKVAL ( StageName , &quot;Closed Won&quot;)
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>You must submit this Opportunity for review prior to closing. To do this, update &quot;Win Validation&quot; to Pending Review.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Win_Validation_SS5_Below</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISNEW()),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.Name&lt;&gt;&quot;Advisory&quot;,
RecordType.Name&lt;&gt;&quot;Advisory - Closed&quot;,
RecordType.Name&lt;&gt;&quot;Favor&quot;,
RecordType.Name&lt;&gt;&quot;Favor - Closed&quot;,
OR(
ISPICKVAL(StageName,&quot;Qualified Prospect (2)&quot;),
ISPICKVAL(StageName,&quot;Conducting Needs Analysis (3)&quot;),
ISPICKVAL(StageName,&quot;Pricing Proposal Sent (4)&quot;),
ISPICKVAL(StageName,&quot;Agreement Sent and Being Negotiated (5)&quot;)),
NOT(ISBLANK(TEXT(Win_Validation__c))))</errorConditionFormula>
        <errorDisplayField>Win_Validation__c</errorDisplayField>
        <errorMessage>Win Validation can only be selected if on SS6 or higher. Please make this field blank for now.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_2_Amount_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
Length_of_Contract__c&lt;=1,
NOT(ISBLANK(X2nd_Year_Amount_in_USD__c)))</errorConditionFormula>
        <errorMessage>You can only enter a Year 2 Amount if the Contract length is more than 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_3_Amount_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
Length_of_Contract__c&lt;=2,
NOT(ISBLANK(X3rd_Year_Amount_in_USD__c)))</errorConditionFormula>
        <errorMessage>You can only enter a Year 3 Amount if the Contract length is more than 2.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_4_Amount_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
Length_of_Contract__c&lt;=3,
NOT(ISBLANK(Year_4_Amount__c)))</errorConditionFormula>
        <errorMessage>You can only enter a Year 4 Amount if the Contract length is more than 3.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_5_Amount_Populated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
Length_of_Contract__c =4,
NOT(ISBLANK(Year_5_Amount__c)))</errorConditionFormula>
        <errorMessage>You can only enter a Year 5 Amount if the Contract length is more than 4.</errorMessage>
    </validationRules>
