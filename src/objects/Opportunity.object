			ROUND(AverageTotalARR__c,2) &lt;&gt;  DDS_ApprovedARR__c
			),
		ISPICKVAL( DDS_ApprovalProcessStatus__c , &quot;Approved&quot;) 
		),
	AND(
		DDS_ScoredARR__c &lt;&gt; ROUND( AverageTotalARR__c ,2),
		NOT(ISBLANK(TEXT(DDS_ScoreAssignment__c)))
	)
	)</formula>
