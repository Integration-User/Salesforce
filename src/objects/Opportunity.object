    <fields>
        <fullName>Pipeline_Category_Sort__c</fullName>
        <externalId>false</externalId>
        <formula>IF(DATE(YEAR(CloseDate),MONTH(CloseDate),1) &lt; DATE(YEAR(TODAY()),MONTH(TODAY()),1),00,

IF(
OR(
DATE(YEAR(CloseDate),MONTH(CloseDate),1) = DATE(YEAR(TODAY()),MONTH(TODAY()),1),
DATE(YEAR(CloseDate),MONTH(CloseDate),1) = DATE(YEAR(TODAY()),MONTH(TODAY())+1,1),
DATE(YEAR(CloseDate),MONTH(CloseDate),1) = DATE(YEAR(TODAY()),MONTH(TODAY())+2,1)),

CASE(MONTH(CloseDate),
1,01,
2,02,
3,03,
4,04,
5,05,
6,06,
7,07,
8,08,
9,09,
10,10,
11,11,
12,12,
null),

IF(YEAR(CloseDate) &gt; YEAR(TODAY()),14,

IF(MONTH(CloseDate)+YEAR(CloseDate) &gt; MONTH(TODAY()+2)+YEAR(TODAY()),13,

NULL))))</formula>
        <label>Pipeline Category Sort</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Pipeline_Category__c</fullName>
        <externalId>false</externalId>
        <formula>IF(DATE(YEAR(CloseDate),MONTH(CloseDate),1) &lt; DATE(YEAR(TODAY()),MONTH(TODAY()),1),&quot;Outdated&quot;,

IF(
OR(
DATE(YEAR(CloseDate),MONTH(CloseDate),1) = DATE(YEAR(TODAY()),MONTH(TODAY()),1),
DATE(YEAR(CloseDate),MONTH(CloseDate),1) = DATE(YEAR(TODAY()),MONTH(TODAY())+1,1),
DATE(YEAR(CloseDate),MONTH(CloseDate),1) = DATE(YEAR(TODAY()),MONTH(TODAY())+2,1)),

CASE(MONTH(CloseDate),
1,&quot;January &quot;,
2,&quot;February &quot;,
3,&quot;March &quot;,
4,&quot;April &quot;,
5,&quot;May &quot;,
6,&quot;June &quot;,
7,&quot;July &quot;,
8,&quot;August &quot;,
9,&quot;September &quot;,
10,&quot;October &quot;,
11,&quot;Novermber &quot;,
12,&quot;December &quot;,
null)+TEXT(YEAR(CloseDate)),

IF(YEAR(CloseDate) &gt; YEAR(TODAY()),&quot;Next Year&quot;,

IF(MONTH(CloseDate)+YEAR(CloseDate) &gt; MONTH(TODAY()+2)+YEAR(TODAY()),&quot;Balance of Year&quot;,

NULL))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
