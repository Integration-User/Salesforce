    <validationRules>
        <fullName>AP_Email_Blank_on_Closed_Won</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.Name &lt;&gt; &quot;Favor&quot;,
RecordType.Name &lt;&gt; &quot;Favour - Closed&quot;,
ISPICKVAL(StageName,&quot;Closed Won&quot;),
ISBLANK(AP_Email__c))</errorConditionFormula>
        <errorDisplayField>AP_Email__c</errorDisplayField>
        <errorMessage>Please ensure AP Email is filled in before marking this Opportunity as Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivationDeactivation_Needs</fullName>
        <active>true</active>
        <description>You must fill out the activation/deactivations needs prior to submitting for review or closing as lost</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.Name = &quot;Renewal&quot;,
ISPICKVAL(Activation_Deactivation_Needs__c , &quot;&quot;),
OR(
ISPICKVAL(StageName, &quot;Closed Lost&quot;),
ISPICKVAL(StageName, &quot;Suspended&quot;),
ISPICKVAL(Win_Validation__c, &quot;Pending Review&quot;),
ISPICKVAL(Win_Validation__c, &quot;Opportunity Reviewed&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>Activation_Deactivation_Needs__c</errorDisplayField>
        <errorMessage>This field must be completed prior to Submitting for review or closing as lost/suspended.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivationDeactivation_Needs_ClosedLost</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.Name = &quot;Sales Opportunity&quot;,
ISPICKVAL(Activation_Deactivation_Needs__c , &quot;&quot;),
ISPICKVAL(StageName, &quot;Closed Lost&quot;),
NOT(ISBLANK(Trial_End_Date__c )))</errorConditionFormula>
        <errorDisplayField>Activation_Deactivation_Needs__c</errorDisplayField>
        <errorMessage>You must designate Activation Needs before closing this trial as lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivationDeactivation_Needs_Sales</fullName>
        <active>true</active>
        <description>You must fill out the activation/deactivations needs prior to submitting for review or closing as lost</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.Name = &quot;Sales Opportunity&quot;,
ISPICKVAL(Activation_Deactivation_Needs__c , &quot;&quot;),
ISPICKVAL(Win_Validation__c, &quot;Pending Review&quot;))</errorConditionFormula>
        <errorDisplayField>Activation_Deactivation_Needs__c</errorDisplayField>
        <errorMessage>This field must be completed prior to Submitting for review or closing as lost/suspended.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Additonal_Fields_Reqd_to_Save_Stage_4</fullName>
        <active>true</active>
        <description>In order to save the Opportunity at a stage 4 or higher, you must include a website on the account.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
OR(
RecordType.Name = &quot;Sales Opportunity&quot;,
RecordType.Name = &quot;Renewal&quot;),
OR(
ISPICKVAL(StageName, &quot;Pricing Proposal Sent (4)&quot;),
ISPICKVAL(StageName, &quot;Agreement Sent and Being Negotiated (5)&quot;),
ISPICKVAL(StageName, &quot;Verbal Win (6)&quot;),
ISPICKVAL(StageName, &quot;Closed Won&quot;)),
ISBLANK(Account.Website))</errorConditionFormula>
        <errorMessage>To save at a Stage 4 or higher, you must have the following Account fields populated: Website</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Lead_Source</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
DATEVALUE(CreatedDate)&gt;=DATE(2021,12,15),
RecordType.Name&lt;&gt;&quot;Advisory&quot;,
RecordType.Name&lt;&gt;&quot;Advisory - Closed&quot;,

OR(

AND(
NOT(ISBLANK(TEXT(Default_Contact__r.LeadSource))),
NOT(ISBLANK(Default_Contact__c)),
TEXT(Default_Contact__r.LeadSource)&lt;&gt;TEXT(LeadSource)),

AND(
NOT(ISBLANK(PRIORVALUE(LeadSource))),
IsChanged(LeadSource),
TEXT(Default_Contact__r.LeadSource)&lt;&gt;TEXT(LeadSource))

))</errorConditionFormula>
        <errorDisplayField>LeadSource</errorDisplayField>
        <errorMessage>You cannot change the lead source from its original designation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Create_Duplicate_Renewal</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
Current_Contract_Year__c =1,
OR(
RecordType.Name=&quot;Sales Opportunity&quot;,
RecordType.Name=&quot;Renewal&quot;),
OR(
ISPICKVAL(StageName,&quot;Verbal Win (6)&quot;),
ISPICKVAL(StageName,&quot;Closed Won&quot;)),
AND(
NOT(ISBLANK(X2nd_Year_Amount_in_USD__c)),
Create_New_Renewal_Opportunity__c=TRUE))</errorConditionFormula>
        <errorMessage>To avoid creating duplicate renewals, please EITHER check &quot;Create New Renewal Opportunity&quot; OR enter amounts in Year 2+ for multi-year deals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Create_Renewal_Opp_Mid_Year</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.Name=&quot;Renewal&quot;,

OR(
ISPICKVAL( Win_Validation__c ,&quot;Pending Review&quot;),
ISPICKVAL(StageName,&quot;Closed Won&quot;)),

Create_New_Renewal_Opportunity__c=TRUE,
Current_Contract_Year__c&lt;&gt;1,
(Contract_End_Date__c-Current_Year_Contract_Start_Date__c)&gt;366)</errorConditionFormula>
        <errorMessage>You cannot create a Renewal Opportunity while in the middle of the Contract.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_once_Closed</fullName>
