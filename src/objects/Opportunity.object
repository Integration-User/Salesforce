IF(ISPICKVAL( Billing_Schedule__c , &quot;In-Arrears – Monthly&quot;), &quot;Monthly&quot;,
IF(ISPICKVAL( Billing_Schedule__c , &quot;One-time&quot;), &quot;One-time&quot;,
