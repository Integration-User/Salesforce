    <validationRules>
        <fullName>DDScore_ApprovedScoreChanges</fullName>
        <active>true</active>
        <description>Prevents change the status and Win Validation from Conducting Needs Analysis (3) or Qualified Prospect (2) to any following open stages, including Closed Won, if the approved score or its ARR has changed.</description>
        <errorConditionFormula>AND(
	OR( 
		RecordType.DeveloperName = &quot;Sales_Opportunity&quot;,
		RecordType.DeveloperName = &quot;Sales_Opportunity_Closed&quot;,	
		RecordType.DeveloperName = &quot;Existing_Business&quot;,
		RecordType.DeveloperName = &quot;Renewal_Closed&quot;
		),
	DDS_ARRChanged__c = TRUE,
	DDS_ScoreApplicability__c = TRUE,
	OR(
		ISCHANGED(StageName),
		ISCHANGED(Win_Validation__c)
		),
	NOT(ISPICKVAL(StageName,&quot;Deactivated&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Closed Lost&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Consolidated&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Suspended&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Qualified Prospect (2)&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Conducting Needs Analysis (3)&quot;))
	)</errorConditionFormula>
        <errorMessage>The approved ARR and score has changed. Please, click on &quot;Calculate Score&quot; and follow the approval instructions before moving the stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DDScore_MissingApproval</fullName>
        <active>true</active>
        <description>Prevents change the status from Conducting Needs Analysis (3) or Qualified Prospect (2) to any following open stages, including Closed Won when the score has not been approved. Allow changes w/o approval when &quot;Lead Source&quot; is &quot;News Subscription Request&quot;</description>
        <errorConditionFormula>AND(
OR(
RecordType.DeveloperName = &quot;Sales_Opportunity&quot;,
RecordType.DeveloperName = &quot;Sales_Opportunity_Closed&quot;,
RecordType.DeveloperName = &quot;Existing_Business&quot;,
RecordType.DeveloperName = &quot;Renewal_Closed&quot;
),
OR(
ISBLANK(TEXT(DDS_ApprovalProcessStatus__c)),
NOT(ISPICKVAL(DDS_ApprovalProcessStatus__c,&quot;Approved&quot;))
),
OR(
ISCHANGED(StageName),
ISCHANGED(Win_Validation__c)
),
DDS_ScoreApplicability__c = TRUE,
NOT(ISPICKVAL(StageName,&quot;Deactivated&quot;)),
NOT(ISPICKVAL(StageName,&quot;Closed Lost&quot;)),
NOT(ISPICKVAL(StageName,&quot;Consolidated&quot;)),
NOT(ISPICKVAL(StageName,&quot;Suspended&quot;)),
NOT(ISPICKVAL(StageName,&quot;Qualified Prospect (2)&quot;)),
NOT(ISPICKVAL(StageName,&quot;Conducting Needs Analysis (3)&quot;)),
OR(
NOT(ISPICKVAL(LeadSource ,&quot;News Subscription Request&quot;))))</errorConditionFormula>
        <errorMessage>Please, follow the approval instructions before moving the stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DDScore_NoScore</fullName>
        <active>true</active>
        <description>Prevents change the status from Conducting Needs Analysis (3) or Qualified Prospect (2) to any following open stages, including Closed Won.</description>
        <errorConditionFormula>AND(
	OR( 
		RecordType.DeveloperName = &quot;Sales_Opportunity&quot;,
		RecordType.DeveloperName = &quot;Sales_Opportunity_Closed&quot;,	
		RecordType.DeveloperName = &quot;Existing_Business&quot;,
		RecordType.DeveloperName = &quot;Renewal_Closed&quot;
		),
	OR(
		ISBLANK(TEXT(DDS_ScoreAssignment__c)),
		CONTAINS(TEXT(DDS_ScoreAssignment__c), &quot;Error&quot;)
		),
	ISCHANGED(StageName),
	DDS_ScoreApplicability__c = TRUE,
	NOT(ISPICKVAL(StageName,&quot;Deactivated&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Closed Lost&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Consolidated&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Suspended&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Qualified Prospect (2)&quot;)),
	NOT(ISPICKVAL(StageName,&quot;Conducting Needs Analysis (3)&quot;))
	)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Please, click on &quot;Calculate Score&quot; and follow the approval instructions before moving the stage.</errorMessage>
    </validationRules>
