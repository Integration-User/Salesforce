    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Approved_Approvers</fullName>
        <active>true</active>
        <description>Only Compliance managers can approve these records</description>
        <errorConditionFormula>AND(
 ISPICKVAL( CAF_Type__c , &quot;Activation&quot;),
    ISCHANGED( Status__c ),
    TEXT(Status__c ) = &quot;Approved&quot;,
    $UserRole.Name &lt;&gt; &quot;Compliance&quot;
)</errorConditionFormula>
        <errorMessage>Only Designated Compliance personnel can approve records.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deactivation_Change_Approvals</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(CAF_Type__c,&quot;Deactivation&quot;),
ISPICKVAL(CAF_Type__c,&quot;Change&quot;)),
TEXT(Status__c ) = &quot;Approved&quot;,
$UserRole.Name &lt;&gt; &quot;Compliance&quot;
)</errorConditionFormula>
        <errorMessage>Only the Compliance team can approve records for Change or Deactivation requests.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>US_Change_Pending_Approval_Reqs</fullName>
        <active>true</active>
        <description>Field required on change CAF&apos;s to move to Pending Approval</description>
        <errorConditionFormula>AND(
 ISPICKVAL( CAF_Type__c , &quot;Change&quot;),
 NOT(ISPICKVAL( Change_Type__c, &quot;Favor Extension&quot;)),
OR(
ISPICKVAL( Status__c , &quot;Pending Approval&quot;),
ISPICKVAL( Status__c , &quot;Approved&quot;)
),
OR(
(  Call_Verification__c  = False),
(  Change_Made__c = False)
))</errorConditionFormula>
        <errorMessage>To move to &quot;Pending Approval&quot;, please fill out all the fields in the compliance section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>US_Favor_Activation_Transition_Reqs</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( CAF_Type__c , &quot;Activation&quot;),
RecordType.Name  =  &quot;US Favor&quot;,
OR(
 ISPICKVAL( Status__c , &quot;Transitioned&quot;), 
 ISPICKVAL( Status__c , &quot;Pending Approval&quot;), 
 ISPICKVAL( Status__c , &quot;Approved&quot;)
),
OR(
ISBLANK( Compliance_Main_Contact__c ),
ISBLANK( Primary_GSA_Contact__c  ),
Docs_added_to_compliance_client_file__c = False,
ISBLANK( OFAC_Check__c )
))</errorConditionFormula>
        <errorMessage>The follow is required to move this record to &quot;Transitioned&quot;: OFAC Check, Docs added to compliance client file, along with internal and External Main Contacts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>US_Sub_Activation_PendingApproval_Reqs</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.Name = &quot;US Subscription&quot;,
ISPICKVAL(  On_Boycott_List__c , &quot;&quot;),
(Verify_OFAC__c = False),
ISPICKVAL( CAF_Type__c , &quot;Activation&quot;),
OR(
ISPICKVAL( Status__c , &quot;Pending Approval&quot;),
ISPICKVAL( Status__c , &quot;Approved&quot;)
))</errorConditionFormula>
        <errorMessage>To Move this record to &quot;Pending Approval&quot; make sure the fields in the Compliance Section are completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>US_Sub_Activation_Transition_Reqs</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( CAF_Type__c , &quot;Activation&quot;),
RecordType.Name  =  &quot;US Subscription&quot;,
OR(
 ISPICKVAL( Status__c , &quot;Transitioned&quot;), 
 ISPICKVAL( Status__c , &quot;Pending Approval&quot;), 
 ISPICKVAL( Status__c , &quot;Approved&quot;)
),
OR(
ISBLANK( Compliance_Main_Contact__c ),
ISBLANK( Account_Name__r.Activation_Date__c ) ||
ISBLANK( Primary_GSA_Contact__c  ),
Docs_added_to_compliance_client_file__c = False,
ISBLANK( OFAC_Check__c )
))</errorConditionFormula>
        <errorMessage>The follow is required to move this record to &quot;Transitioned&quot;: OFAC Check, Docs added to compliance client file, along with internal and External Main Contacts. *You also much activate the account prior to transitioning.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
