<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <acrossGroupingContext>CLOSE_DATE</acrossGroupingContext>
        <calculatedFormula>(Opportunity.Same_Store_Amount__c:SUM- (PREVGROUPVAL(Opportunity.Same_Store_Amount__c:SUM, CLOSE_DATE)))/PREVGROUPVAL(Opportunity.Same_Store_Amount__c:SUM, CLOSE_DATE)</calculatedFormula>
