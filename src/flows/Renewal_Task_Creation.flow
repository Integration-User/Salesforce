    <formulas>
        <description>Auto Renewal</description>
        <name>formAutorenewal60</name>
        <dataType>String</dataType>
        <expression>&quot;Upcoming Automatic Renewal + &quot; + {!VAR_AccountInfo.Name}</expression>
    </formulas>
    <formulas>
        <description>Difference Between the Close Date &amp; Today to generate number of days before Close Date</description>
        <name>formDiffCloseToday</name>
        <dataType>Number</dataType>
        <expression>{!$Record.CloseDate} - {!$Flow.CurrentDate}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Today plus 7</description>
        <name>formTodayplus7</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate}+7</expression>
    </formulas>
