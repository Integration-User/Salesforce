    <recordUpdates>
        <name>Case_Due_Date_Update</name>
        <label>Case Due Date Update</label>
        <locationX>578</locationX>
        <locationY>432</locationY>
        <filterLogic>(1 OR 2) AND (3 AND 4)</filterLogic>
        <filters>
            <field>ActivationDeactivation_Needs__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Limited Trial Deactivation</stringValue>
            </value>
        </filters>
        <filters>
            <field>ActivationDeactivation_Needs__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Full Trial Deactivation</stringValue>
            </value>
        </filters>
        <filters>
            <field>Status</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>Closed</stringValue>
            </value>
        </filters>
        <filters>
            <field>Opportunity__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Due_Date__c</field>
            <value>
