    <formulas>
        <name>fDefaultStartDateTime</name>
        <dataType>DateTime</dataType>
        <expression>IF(DATETIMEVALUE({!Get_Subscription.Start_Date__c}) &gt; {!$Flow.CurrentDateTime}, DATETIMEVALUE({!Get_Subscription.Start_Date__c}), {!$Flow.CurrentDateTime} )</expression>
    </formulas>
    <formulas>
        <name>fSubscriptionEndDateTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(TEXT(DATEVALUE(TEXT({!End_Date}))) + &apos; 23:59:59&apos;)</expression>
    </formulas>
    <formulas>
        <name>fSubscriptionStartDateTime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(TEXT(DATEVALUE(TEXT({!Start_Date}))) + &apos; 00:00:00&apos;)</expression>
    </formulas>
    <formulas>
        <name>subscriberStatus</name>
        <dataType>String</dataType>
        <expression>IF({!Start_Date} &gt; TODAY(), &quot;Inactive&quot;, &quot;Active&quot;)</expression>
    </formulas>
